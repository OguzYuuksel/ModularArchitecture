extension _PackageTests {
    
    func testResourcesBundlePath() {
        XCTAssertTrue(resourcePath.contains("RootScreen_RootScreen_Resources_iOS.bundle"))
    }
    
    func testOS() {
        XCTAssertEqual(_os, .iOS)
    }
    
    func testDevice() {
        XCTAssertTrue(_device == .iPad || _device == .iPhone)
    }
    
}
